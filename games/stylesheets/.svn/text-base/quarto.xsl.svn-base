		<style type="text/css" media="all">			#cell			{				width:  40px;				height: 40px;				float:	left;				border: 2px solid #FFC;				background-color: #CCCCCC;			}		</style>		<!-- Draw Board -->		<div id="main" style="position:relative; width:176px; height:176px;">			<div id="cell"/><div id="cell"/><div id="cell"/><div id="cell"/>			<div id="cell"/><div id="cell"/><div id="cell"/><div id="cell"/>			<div id="cell"/><div id="cell"/><div id="cell"/><div id="cell"/>			<div id="cell"/><div id="cell"/><div id="cell"/><div id="cell"/>				<xsl:for-each select="fact[relation='cell' and argument[3]!='empty']">				<xsl:variable name="x" select="44 * (./argument[1]-1)"/>				<xsl:variable name="y" select="44 * (4-./argument[2])"/>					<xsl:variable name="bit3" select="substring(./argument[3],2,1)"/>				<xsl:variable name="bit2" select="substring(./argument[3],3,1)"/>				<xsl:variable name="bit1" select="substring(./argument[3],4,1)"/>				<xsl:variable name="bit0" select="substring(./argument[3],5,1)"/>					<div>									<xsl:if test="$bit3='0' and $bit2='0'">						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:40px;height:40px;border: 2px solid #333333;background-color: #666666;')"/>						</xsl:attribute>														</xsl:if>					<xsl:if test="$bit3='0' and $bit2='1'">						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:40px;height:40px;border: 2px solid #333333;background-color: #999999;')"/>						</xsl:attribute>														</xsl:if>					<xsl:if test="$bit3='1' and $bit2='0'">						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:40px;height:40px;border: 2px dashed #333333;background-color: #666666;')"/>						</xsl:attribute>														</xsl:if>					<xsl:if test="$bit3='1' and $bit2='1'">						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:40px;height:40px;border: 2px dashed #333333;background-color: #999999;')"/>						</xsl:attribute>														</xsl:if>						<xsl:if test="$bit1='0' and $bit0='0'">						<img src="/docserver/gamemaster/images/ghost.gif"/>					</xsl:if>					<xsl:if test="$bit1='0' and $bit0='1'">						<img src="/docserver/gamemaster/images/blackGhost.gif"/>					</xsl:if>					<xsl:if test="$bit1='1' and $bit0='0'">						<img src="/docserver/gamemaster/images/indiana.gif"/>					</xsl:if>					<xsl:if test="$bit1='1' and $bit0='1'">						<img src="/docserver/gamemaster/images/blueMan.gif"/>					</xsl:if>				</div>						</xsl:for-each>		</div>				<div id="spacer" style="width:176px; height:10px"/>				<div style="position:relative; width:176px; height:176px;">			<xsl:for-each select="fact[relation='pool']">				<xsl:variable name="bit3" select="substring(./argument[1],2,1)"/>				<xsl:variable name="bit2" select="substring(./argument[1],3,1)"/>				<xsl:variable name="bit1" select="substring(./argument[1],4,1)"/>				<xsl:variable name="bit0" select="substring(./argument[1],5,1)"/>				<div>					<xsl:if test="$bit3='0' and $bit2='0'">						<xsl:attribute name="style">							<xsl:value-of select="'float:left; width:40px; height:40px; border:2px solid #333333; background-color: #666666;'"/>						</xsl:attribute>													</xsl:if>					<xsl:if test="$bit3='0' and $bit2='1'">						<xsl:attribute name="style">							<xsl:value-of select="'float:left; width:40px;height:40px;border: 2px solid #333333;background-color: #999999;'"/>						</xsl:attribute>					</xsl:if>					<xsl:if test="$bit3='1' and $bit2='0'">						<xsl:attribute name="style">							<xsl:value-of select="'float:left; width:40px;height:40px;border: 2px dashed #333333;background-color: #666666;'"/>						 </xsl:attribute>						</xsl:if>					<xsl:if test="$bit3='1' and $bit2='1'">						<xsl:attribute name="style">							<xsl:value-of select="'float:left; width:40px;height:40px;border: 2px dashed #333333;background-color: #999999;'"/>												</xsl:attribute>						</xsl:if>							<xsl:if test="$bit1='0' and $bit0='0'">						<img src="/docserver/gamemaster/images/ghost.gif"/>					</xsl:if>					<xsl:if test="$bit1='0' and $bit0='1'">						<img src="/docserver/gamemaster/images/blackGhost.gif"/>					</xsl:if>					<xsl:if test="$bit1='1' and $bit0='0'">						<img src="/docserver/gamemaster/images/indiana.gif"/>					</xsl:if>					<xsl:if test="$bit1='1' and $bit0='1'">						<img src="/docserver/gamemaster/images/blueMan.gif"/>					</xsl:if>				</div>				</xsl:for-each>		</div>			