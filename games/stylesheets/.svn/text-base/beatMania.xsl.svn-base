				<style type="text/css" media="all">					#cell					{						width:  46px;						height: 21px;						float:	left;						border: 2px solid #FFC;						background-color: #FFFFFF;					}				</style>				<div id="main" style="position:relative; width:150px; height:780px">					<!-- Draw Board -->					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>					<div id="cell"/><div id="cell"/><div id="cell"/>										<!-- Draw Blocks -->					<xsl:for-each select="fact[relation='block']">						<xsl:variable name="x" select="./argument[1]"/>						<xsl:variable name="y" select="./argument[2]"/>													<xsl:variable name="xPos" select="2 + ($x - 1) * 50"/>						<xsl:variable name="yPos" select="2 + (30 - $y) * 25"/>											<xsl:if test="$y = 1">							<div>														<xsl:attribute name="style">									<xsl:value-of select="concat('position:absolute; left:', $xPos ,'px; top:', $yPos ,'px; width:46px; height:21px; background-color: #FF0000')"/>								</xsl:attribute>									</div>											</xsl:if>						<xsl:if test="$y != 1">							<div>														<xsl:attribute name="style">									<xsl:value-of select="concat('position:absolute; left:', $xPos ,'px; top:', $yPos ,'px; width:46px; height:21px; background-color: #CCCCCC')"/>								</xsl:attribute>									</div>											</xsl:if>					</xsl:for-each>									<!-- Write catch count -->					<div style="text-align:center">						<xsl:value-of select="fact[relation='blockscaught']/argument[1]"/>						<xsl:text> </xsl:text>						pieces caught					</div>				</div>						