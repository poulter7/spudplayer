		<style type="text/css" media="all">			#cell			{				width:  36px;				height: 36px;				float:	left;				border: 2px solid #FFC;				background-color: #CCCCCC;			}			#wall			{				width:  36px;				height: 36px;				float:	left;				border: 2px solid #FFC;				background-color: #000000;			}			#redhead			{				width:            36px;				height:           36px;				background-color: #FF0000;			}			#bluehead			{				width:            36px;				height:           36px;				background-color: #0000FF;			}			#fill			{				width:            36px;				height:           36px;				background-color: #99CCFF;			}		</style>		<!-- Draw board -->		<div id="main" style="position:relative; width:240px; height:240px">			<div id="wall"/><div id="wall"/><div id="wall"/><div id="wall"/><div id="wall"/><div id="wall"/>			<div id="wall"/><div id="cell"/><div id="cell"/><div id="cell"/><div id="cell"/><div id="wall"/>			<div id="wall"/><div id="cell"/><div id="cell"/><div id="cell"/><div id="cell"/><div id="wall"/>			<div id="wall"/><div id="cell"/><div id="cell"/><div id="cell"/><div id="cell"/><div id="wall"/>			<div id="wall"/><div id="cell"/><div id="cell"/><div id="cell"/><div id="cell"/><div id="wall"/>			<div id="wall"/><div id="wall"/><div id="wall"/><div id="wall"/><div id="wall"/><div id="wall"/>			<!-- Color selected cells -->			<xsl:for-each select="fact[relation='cell']">				<xsl:if test="./argument[3]='f'">										<div id="fill">						<xsl:variable name="x" select="2 + (./argument[1])*40"/>						<xsl:variable name="y" select="2 + (./argument[2])*40"/>												<xsl:attribute name="style">							<xsl:value-of select="concat('width:36px; height:36px; position:absolute; left:', $x ,'px; top:', $y ,'px;')"/>													</xsl:attribute>																	</div>					</xsl:if>								<xsl:if test="./argument[3]='redhead'">										<div id="redhead">						<xsl:variable name="x" select="2 + (./argument[1])*40"/>						<xsl:variable name="y" select="2 + (./argument[2])*40"/>												<xsl:attribute name="style">							<xsl:value-of select="concat('width:36px; height:36px; position:absolute; left:', $x ,'px; top:', $y ,'px;')"/>													</xsl:attribute>																	</div>					</xsl:if>								<xsl:if test="./argument[3]='bluehead'">										<div id="bluehead">						<xsl:variable name="x" select="2 + (./argument[1])*40"/>						<xsl:variable name="y" select="2 + (./argument[2])*40"/>												<xsl:attribute name="style">							<xsl:value-of select="concat('width:36px; height:36px; position:absolute; left:', $x ,'px; top:', $y ,'px;')"/>													</xsl:attribute>																	</div>					</xsl:if>			</xsl:for-each>		</div>			