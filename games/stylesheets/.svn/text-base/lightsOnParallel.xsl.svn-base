		<style type="text/css" media="all">			#cellL			{				width:  46px;				height: 46px;				float:	left;				border: 2px solid #FFC;				background-color: #CCCCCC;			}			#cellD			{				width:  46px;				height: 46px;				float:	left;				border: 2px solid #FFC;				background-color: #AAAAAA;			}			</style>				<div id="main" style="position:relative; width:200px; height:200px">			<!-- Draw Board -->			<div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellD"/>			<div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellD"/>			<div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellD"/>			<div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellD"/>					<!-- Draw tokens -->			<xsl:for-each select="fact[relation='cell']">				<xsl:variable name="x" select="50 * (./argument[1]-1)"/>				<xsl:variable name="y" select="50 * (4-./argument[2])"/>								<xsl:if test="./argument[3]='1'">					<div>						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:46px; height:46px; border:2px solid #FFC; background-color: #660000;')"/>						</xsl:attribute>							</div>				</xsl:if>										</xsl:for-each>			<xsl:for-each select="fact[relation='cell']">				<xsl:variable name="x" select="50 * (./argument[1]-1)"/>				<xsl:variable name="y" select="50 * (4-./argument[2])"/>								<xsl:if test="./argument[3]='2'">					<div>						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:46px; height:46px; border:2px solid #FFC; background-color: #666600;')"/>						</xsl:attribute>							</div>				</xsl:if>										</xsl:for-each>			<xsl:for-each select="fact[relation='cell']">				<xsl:variable name="x" select="50 * (./argument[1]-1)"/>				<xsl:variable name="y" select="50 * (4-./argument[2])"/>								<xsl:if test="./argument[3]='3'">					<div>						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:46px; height:46px; border:2px solid #FFC; background-color: #669900;')"/>						</xsl:attribute>							</div>				</xsl:if>										</xsl:for-each>			<xsl:for-each select="fact[relation='cell']">				<xsl:variable name="x" select="50 * (./argument[1]-1)"/>				<xsl:variable name="y" select="50 * (4-./argument[2])"/>								<xsl:if test="./argument[3]='4'">					<div>						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:46px; height:46px; border:2px solid #FFC; background-color: #66FF00;')"/>						</xsl:attribute>							</div>				</xsl:if>										</xsl:for-each>		</div>		