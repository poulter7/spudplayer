		<style type="text/css" media="all">			#cellL			{				width:  46px;				height: 46px;				float:	left;				border: 2px solid #FFC;				background-color: #CCCCCC;			}			#cellD			{				width:  46px;				height: 46px;				float:	left;				border: 2px solid #FFC;				background-color: #AAAAAA;			}			#cellE			{				width:  46px;				height: 46px;				float:	left;				border: 2px solid #FFC;				background-color: #FFC;			}		</style>		<!-- Draw Board -->		<div id="main" style="position:relative; width:350px; height:300px">			<div id="cellE"/><div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellE"/>			<div id="cellL"/><div id="cellD"/><div id="cellL"/><div id="cellL"/><div id="cellL"/><div id="cellD"/><div id="cellL"/>			<div id="cellL"/><div id="cellD"/><div id="cellL"/><div id="cellL"/><div id="cellL"/><div id="cellD"/><div id="cellL"/>			<div id="cellL"/><div id="cellD"/><div id="cellL"/><div id="cellL"/><div id="cellL"/><div id="cellD"/><div id="cellL"/>			<div id="cellE"/><div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellD"/><div id="cellE"/>			<div id="cellE"/><div id="cellE"/><div id="cellL"/><div id="cellL"/><div id="cellL"/><div id="cellE"/><div id="cellE"/>			<!-- Draw Marks -->			<xsl:for-each select="fact[relation='cell']">				<xsl:variable name="x" select="50 * (./argument[1]-1)"/>				<xsl:variable name="y" select="50 * (6-./argument[2])"/>								<div>					<xsl:if test="substring(./argument[3],1,3)='red'">						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:46px; height:46px; border: 2px solid #FFC; background-color:#CC0000')"/>						</xsl:attribute>							</xsl:if>					<xsl:if test="substring(./argument[3],1,6)='yellow'">						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:46px; height:46px; border: 2px solid #FFC; background-color:#CCCC00')"/>						</xsl:attribute>							</xsl:if>					<xsl:if test="substring(./argument[3],1,4)='blue'">						<xsl:attribute name="style">							<xsl:value-of select="concat('position:absolute; left:', $x ,'px; top:', $y ,'px; width:46px; height:46px; border: 2px solid #FFC; background-color:#6699CC')"/>						</xsl:attribute>							</xsl:if>					<xsl:if test="./argument[3]='reddisc'">						<img src="/docserver/gamemaster/images/bcc.gif"/>					</xsl:if>					<xsl:if test="./argument[3]='yellowdisc'">						<img src="/docserver/gamemaster/images/bcc.gif"/>					</xsl:if>											<xsl:if test="./argument[3]='bluedisc'">						<img src="/docserver/gamemaster/images/bcc.gif"/>					</xsl:if>											<xsl:if test="./argument[3]='redpawn'">						<img src="/docserver/gamemaster/images/wpawn.gif"/>					</xsl:if>					<xsl:if test="./argument[3]='yellowpawn'">						<img src="/docserver/gamemaster/images/wpawn.gif"/>					</xsl:if>											<xsl:if test="./argument[3]='bluepawn'">						<img src="/docserver/gamemaster/images/wpawn.gif"/>					</xsl:if>											<xsl:if test="./argument[3]='redknight'">						<img src="/docserver/gamemaster/images/wn.gif"/>					</xsl:if>					<xsl:if test="./argument[3]='yellowknight'">						<img src="/docserver/gamemaster/images/wn.gif"/>					</xsl:if>											<xsl:if test="./argument[3]='blueknight'">						<img src="/docserver/gamemaster/images/wn.gif"/>					</xsl:if>											<xsl:if test="./argument[3]='redchecker'">						<img src="/docserver/gamemaster/images/bcking.png"/>					</xsl:if>					<xsl:if test="./argument[3]='yellowchecker'">						<img src="/docserver/gamemaster/images/bcking.png"/>					</xsl:if>											<xsl:if test="./argument[3]='bluechecker'">						<img src="/docserver/gamemaster/images/bcking.png"/>					</xsl:if>															</div>							</xsl:for-each>		</div>		