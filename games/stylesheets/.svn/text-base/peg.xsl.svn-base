		<style type="text/css" media="all">			#main			{				position:   relative;				width:      370px;				height:     350px;				padding:    5px;				border:     2px solid #b17735;				background: transparent url(/docserver/gamemaster/images/ccboard.gif) repeat top left; 			}								#AC4,#AC5,#BC4,#BC5,#CC2,#CC3,#CC4,#CC5,#CC6,#CC7,#DC2,#DC3,#DC4,#DC5,#DC6,#DC7,#EC2,#EC3,#EC4,#EC5,#EC6,#EC7,#FC4,#FC5,#GC4,#GC5			{				float: left;			}			#AC3,#BC3,#FC3,#GC3			{				margin-left: 110px;				float: left;				clear: left;			}			#CC1,#DC1,#EC1			{				margin-left: 10px;				float: left;				clear: left;			}			div.peg			{				width: 50px;				height: 50px;			}			div.empty			{				width: 50px;				height: 50px;			}		</style>		<div id="main">			<xsl:for-each select="fact[relation='hole']">				<xsl:sort select="argument[1]"/>				<xsl:sort select="argument[2]"/>				<xsl:choose>					<xsl:when test="argument[3]='peg'">						<div class="peg">							<xsl:attribute name="id"><xsl:value-of select="argument[1]"/><xsl:value-of select="argument[2]"/></xsl:attribute>							<img src="/docserver/gamemaster/images/tophat.gif"/>						</div>					</xsl:when>					<xsl:otherwise>						<div class="empty">							<xsl:attribute name="id"><xsl:value-of select="argument[1]"/><xsl:value-of select="argument[2]"/></xsl:attribute>							<img src="/docserver/gamemaster/images/ring.gif"/>						</div>					</xsl:otherwise>				</xsl:choose>			</xsl:for-each>		</div>		